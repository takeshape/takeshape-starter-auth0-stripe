      "resolver": {
        "compose": [
          {
            "if": "!isEmpty($claims.sub)",
            "id": "profile",
            "name": "takeshape:find",
            "service": "takeshape:local",
            "shapeName": "Profile",
            "args": { "ops": [{ "path": "where.id.eq", "mapping": "$claims.sub" }] }
